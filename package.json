  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "generate:prerender": "cd dist && node prerender",
    "webpack:server:node": "webpack --config webpack.server.node.config.js --progress --colors",
    "webpack:browser": "webpack --config webpack.browser.config.js -p --progress",
    "webpack:server": "webpack --config webpack.server.config.js -p --progress",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "webpack:ts-node": "webpack --config webpack.ts-js.server.config.js --progress --colors",
    "server-dev": "cross-env NODE_ENV=development nodemon server.js",
    "server-prod": "cross-env NODE_ENV=production nodemon server.js"
  },
  "private": true,
